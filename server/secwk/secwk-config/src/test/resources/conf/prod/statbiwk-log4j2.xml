<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="trace">
	<Properties>
		<Property name="emitterName">statbiwk</Property>
		<Property name="emitterVersion">0.0.30</Property>
		<Property name="emitterCodeService">statbi</Property>
	</Properties>
	<Appenders>
        <Console name="console">
            <PatternLayout>
                <Pattern>%m%n</Pattern>
            </PatternLayout>
        </Console>
        <Console name="filer">
            <PatternLayout>
                <Pattern>FILE : %m%n</Pattern>
            </PatternLayout>
        </Console>
	</Appenders>
	<Loggers>
        <Root level="info">
            <AppenderRef ref="console" />
        </Root>
        <Logger level="trace" name="filer">
            <AppenderRef ref="filer" />
        </Logger>
	</Loggers>
</Configuration>
